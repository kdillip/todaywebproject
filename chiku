//MULTI STAGE PIPELINE AS A CODE: we can write multiple commands on multiple stage

pipeline {
    agent any 
    
    stages {
        stage("one") {
            steps {
                sh 'touch file2'
                sh 'lsmem'
                sh 'lscpu'
            }
        }
        stage("two") {
            steps {
                sh 'uname'
                sh 'hostname -i'
            }
        }
    }
}
